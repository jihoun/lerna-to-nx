{
  "name": "@jihoun/lerna-to-nx",
  "version": "0.0.1",
  "description": "An angular schematics that migrates a repository from lerna to nx",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json --watch",
    "test": "npm run build && jasmine src/**/*_spec.js"
  },
  "keywords": [
    "schematics",
    "lerna",
    "nx"
  ],
  "author": "",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^9.1.0",
    "@angular-devkit/schematics": "^9.1.0",
    "@schematics/angular": "^9.1.0",
    "glob": "^7.1.6",
    "typescript": "~3.8.2"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/jasmine": "~3.5.0",
    "@types/node": "^12.11.1",
    "jasmine": "^3.5.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  }
}
